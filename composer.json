{
    "name": "nzmebooks/craft-event-helper",
    "description": "Event Helper is a simple Craft CMS plugin that gives you the ability to track event attendance.",
    "type": "craft-plugin",
    "version": "3.1.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "event-helper"
    ],
    "support": {
        "docs": "https://github.com/nzmebooks/craft-event-helper/blob/master/README.md",
        "issues": "https://github.com/nzmebooks/craft-event-helper/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "meBooks",
            "homepage": "https://mebooks.co.nz"
        }
    ],
    "repositories": [
        {
        "type": "git",
        "url": "https://github.com/nzmebooks/craft-plain-ics"
        }
    ],
    "require": {
        "craftcms/cms": "^5.0.0-beta.1",
        "php": "^8.2",
        "plainlanguage/craft-plain-ics": "^3.0"
    },
    "autoload": {
        "psr-4": {
          "nzmebooks\\eventhelper\\": "src/"
        }
    },
    "extra": {
        "name": "Event Helper",
        "handle": "event-helper",
        "hasCpSettings": false,
        "hasCpSection": true,
        "changelogUrl": "https://raw.githubusercontent.com/nzmebooks/craft-event-helper/master/CHANGELOG.md",
        "class": "nzmebooks\\eventhelper\\EventHelper"
    },
    "config": {
        "allow-plugins": {
            "craftcms/plugin-installer": true,
            "yiisoft/yii2-composer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "craftcms/rector": "dev-main"
    }
}
